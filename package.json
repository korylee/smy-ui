{
  "name": "smy-h5",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "reinstall": "rimraf node_modules",
    "build": "node scripts/build.mjs",
    "changelog": "smy-h5-cli changelog",
    "lint": "smy-h5-cli lint",
    "release": "smy-h5-cli update-version && node scripts/build.mjs && smy-h5-cli publish ui icons repl"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --allow-empty --concurrent false",
    "commit-msg": "npx --no-install smy-h5-cli lint-commit $1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,vue,less}": "prettier --write",
    "*.{ts,tsx,js,vue}": "eslint --fix"
  },
  "devDependencies": {
    "@smy-h5/cli": "workspace:*",
    "@smy-h5/eslint-config": "workspace:*",
    "@types/node": "^16.11.45",
    "execa": "5.1.1",
    "lint-staged": "^14.0.1",
    "nanospinner": "^1.1.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^14.11.0",
    "typescript": "~5.2.2"
  },
  "packageManager": "pnpm@7.8.0"
}
