{
  "name": "@smy-h5/ui",
  "version": "0.0.1",
  "module": "es/index.js",
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "unpkg": "dist/index.js",
  "vetur": {
    "tags": "highlight/tags.json",
    "attributes": "highlight/attributes.json"
  },
  "exports": {
    ".": {
      "import": "./es/index.js",
      "require": "./lib/index.js"
    }
  },
  "keywords": [
    "vue2",
    "UI"
  ],
  "files": [
    "lib",
    "es",
    "umd",
    "types",
    "lighlight"
  ],
  "scripts": {
    "dev": "smy-h5-cli dev",
    "build": "smy-h5-cli build",
    "compile": "smy-h5-cli compile",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false"
  },
  "devDependencies": {
    "@smy-h5/cli": "workspace:*",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.45",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/test-utils": "^1.3.0",
    "@vue/tsconfig": "^0.1.3",
    "conventional-changelog": "^3.1.25",
    "jsdom": "^20.0.0",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "terser": "^5.14.2",
    "typescript": "~4.7.4",
    "vite": "^3.0.8",
    "vitest": "^0.18.1",
    "vue": "2.6.14",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "2.6.14",
    "vue-tsc": "^0.38.8"
  },
  "peerDependencies": {
    "vue": ">= 2.6.0"
  },
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "es/style.js",
    "es/less.js",
    "lib/style.js",
    "lib/less.js",
    "*.css",
    "*.less"
  ],
  "browserslist": [
    "Chrome >= 51",
    "iOS >= 10"
  ]
}
